{
  "name": "livingdocs-website-frontend",
  "private": true,
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/livingdocsIO/livingdocs-website-frontend.git"
  },
  "scripts": {
    "design:dev": "NODE_ENV=development npm run start:website",
    "design:dev:mocked": "NODE_ENV=development run-p start:json-server start:website:watch",
    "design:build": "NODE_ENV=production webpack --display-error-details",
    "design:publish": "DESIGN_TARGET=editor npm run design:build && ldm design:publish design/dist",
    "start:json-server": "nodemon --watch ./mocks --exec 'json-server ./mocks/db.js --routes ./mocks/routes.json --port 3001'",
    "start:website": "node app/index.js",
    "start:website:watch": "nodemon --watch conf --watch lib --watch app app/index.js",
    "start:website:mocked": "run-p start:json-server start:website",
    "start": "npm run design:dev:mocked"
  },
  "devDependencies": {
    "animejs": "^2.2.0",
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.11.6",
    "chartist": "^0.11.0",
    "chartist-plugin-legend": "^0.6.2",
    "chokidar": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.7",
    "gifffer": "^1.5.1",
    "html-loader": "^0.4.5",
    "html-minifier": "^0.5.6",
    "imports-loader": "^0.8.0",
    "intersection-observer": "^0.5.0",
    "jquery": "^3.2.1",
    "json-server": "^0.12.0",
    "list.js": "^1.5.0",
    "livingdocs-manager": "^4.1.0",
    "mark.js": "^8.11.1",
    "node-sass": "^4.5.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.0.6",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.1",
    "scrollama": "^1.4.1",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.1.0",
    "webpack-hot-middleware": "^2.21.2"
  },
  "dependencies": {
    "@livingdocs/conf": "^1.0.1",
    "@livingdocs/node-sdk": "^0.3.2",
    "body-parser": "^1.18.2",
    "combined-slugify": "^2.1.0",
    "compression": "^1.7.1",
    "express": "^4.1.1",
    "handlebars": "^4.0.11",
    "node-fetch": "^2.1.2",
    "uuid": "^3.2.1"
  },
  "release": {
    "extends": "@livingdocs/semantic-release-presets/github"
  }
}
